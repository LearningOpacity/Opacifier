{
  "name": "opacifier",
  "version": "1.0.0",
  "description": "An Opacifier",
  "main": "src/index.js",
  "directories": {
    "doc": "doc"
  },
  "type": "module",
  "scripts": {
    "build": "standard --fix && npm run lint-fix && npm run gen-doc && npm run test",
    "gen-doc": "jsdoc src/*",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "test": "jest \"test/*.js\""
  },
  "standard": {
    "env": [ "jest" ]
  },
  "jest": {
    "testMatch": [
        "<rootDir>/test/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LearningOpacity/Opacifier.git"
  },
  "author": "Evan Plett",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LearningOpacity/Opacifier/issues"
  },
  "homepage": "https://github.com/LearningOpacity/Opacifier#readme",
  "dependencies": {
    "express": "^4.17.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.0.1",
    "jsdoc": "^3.6.4",
    "standard": "^14.3.4"
  }
}
