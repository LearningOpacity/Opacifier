{
  "name": "opacifier",
  "version": "1.0.0",
  "description": "An Opacifier",
  "main": "src/index.js",
  "directories": {
    "doc": "doc"
  },
  "type": "module",
  "scripts": {
    "verify": "standard --fix && eslint src --fix",
    "gen-doc": "jsdoc src/*",
    "test-unit": "jest --group=unit",
    "test-integration": "jest --group=integration",
    "todo": "leasot -x 'src/*.js' 'test/**/*.js'",
    "build": "npm run verify && npm run gen-doc && npm run test-unit && npm run test-integration",
    "devbuild": "npm run verify && npm run test-unit -- -o && npm run todo"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LearningOpacity/Opacifier.git"
  },
  "author": "Evan Plett",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LearningOpacity/Opacifier/issues"
  },
  "homepage": "https://github.com/LearningOpacity/Opacifier#readme",
  "dependencies": {
    "abort-controller": "^3.0.0",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "iconv": "^3.0.0",
    "js-yaml": "^3.14.0",
    "node-fetch": "^2.6.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.0.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-runner-groups": "^2.0.1",
    "jsdoc": "^3.6.4",
    "leasot": "^11.0.0",
    "nock": "^12.0.3",
    "standard": "^14.3.4",
    "supertest": "^4.0.2"
  }
}
